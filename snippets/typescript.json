    "draw_circle": {
        "prefix": "draw_circle",
        "body": [
            "var shape:egret.Shape = new egret.Shape();",
            "shape.graphics.beginFill(0xff0000);",
            "shape.graphics.drawCircle(0,0,200);",
            "shape.graphics.endFill();",
            "this.addChild(shape);",
            "$1"
        ],
        "description": "生成圆形"
    },
    "draw_rect": {
        "prefix": "draw_rect",
        "body": [
            "var shape:egret.Shape = new egret.Shape();",
            "shape.graphics.beginFill(0x00ff00);",
            "shape.graphics.drawRect(0,0,200,200);",
            "shape.graphics.endFill();",
            "this.addChild(shape);",
            "$1"
        ],
        "description": "生成矩形"
    },
    "tween": {
        "prefix": "tween",
        "body": [
            "egret.Tween.get($1).to({x:20},500,egret.Ease.bounceIn);",
            "$1"
        ],
        "description": "生成Tween"
    }

