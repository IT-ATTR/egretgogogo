{
    "fori": {
        "prefix": "fori",
        "body": [
            "for(var i:number = 0;i<${1:array}.length;i++){\n\n\n}"
        ],
        "description": "生成fori循环"
    },
    "forij": {
        "prefix": "forij",
        "body": [
           "for(var i:number = 0;i<${1:array}.length;i++){\n for(var j:number = 0;j<${2:array}.length;j++){\n $3\n}"
        ],
        "description": "生成fori-j循环"
    },
     "button": {
        "prefix": "button",
        "body": [
            "var button:eui.Button = new eui.Button();",
            "$1"
        ],
        "description": "生成Button"
    },
   "button-click": {
        "prefix": "button-click",
        "body": [
                "var button:eui.Button = new eui.Button();",
                "button.label = 'Test';",
                "button.width = 200;",
                "button.height = 150;",
                "this.addChild(button);",
                "button.addEventListener(egret.TouchEvent.TOUCH_TAP,(event:TouchEvent)=>{",
                "\n$1",
                "},this);" 
        ],
        "description": "生成Button点击"
    },
  
    "textField": {
        "prefix": "textfield",
        "body": [
            "var textfield:egret.TextField = new egret.TextField();",
            "this.addChild(textfield);",
            "$1"
        ],
        "description": "生成TextField"
    },  
    "bitmap": {
        "prefix": "bitmap",
        "body": [
            "var  bitmap:egret.Bitmap = new egret.Bitmap();",
             "this.addChild(bitmap);",
            "$1"
        ],
        "description": "生成Bitmap"
    },  
    "image": {
        "prefix": "image",
        "body": [
            "var  image:eui.Image = new eui.Image();",
            "image.source = 'example_png';",
            "this.addChild(image);",
            "$1"
        ],
        "description": "生成image"
    },  
    
    "radioButton": {
